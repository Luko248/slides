---
import Slide from "@presentation/components/slide/Slide.astro";

interface Props {
  order?: number;
}

const { order } = Astro.props;
---

<Slide order={order} center>
  <Fragment slot="title">
    Konzistentné AI workflow naprieč vývojovými tímami
  </Fragment>
  <strong class="text-center text-theme-500 mx-auto block text-5xl" style="font-family: 'Pixelify Sans', sans-serif !important;">
    Cesta od vibe codingu k AI-first vývoju v AIS Servis
  </strong>
</Slide>
